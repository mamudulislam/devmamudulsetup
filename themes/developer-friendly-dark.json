{
  "name": "Developer Friendly Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e2e",
    "editor.foreground": "#cdd6f4",
    "editor.lineHighlightBackground": "#313244",
    "editorLineNumber.foreground": "#6c7086",
    "editorLineNumber.activeForeground": "#fab387",
    "editorCursor.foreground": "#f38ba8",
    "editor.selectionBackground": "#585b70",
    "editor.inactiveSelectionBackground": "#45475a",
    "editorWhitespace.foreground": "#6c7086",
    "editorIndentGuide.background": "#6c7086",
    "editorIndentGuide.activeBackground": "#9399b2",
    
    "errorForeground": "#f38ba8",
    "warningForeground": "#fab387",
    "infoForeground": "#89b4fa",
    
    "sideBar.background": "#181825",
    "sideBar.foreground": "#cdd6f4",
    "sideBarTitle.foreground": "#f9e2af",
    "sideBarSectionHeader.background": "#11111b",
    
    "activityBar.background": "#11111b",
    "activityBar.foreground": "#cdd6f4",
    "activityBar.activeBackground": "#89b4fa",
    "activityBar.activeForeground": "#1e1e2e",
    
    "tab.activeBackground": "#313244",
    "tab.inactiveBackground": "#181825",
    "tab.activeForeground": "#cdd6f4",
    "tab.inactiveForeground": "#6c7086",
    
    "statusBar.background": "#11111b",
    "statusBar.foreground": "#cdd6f4",
    "statusBar.noFolderBackground": "#11111b",
    
    "titleBar.activeBackground": "#11111b",
    "titleBar.activeForeground": "#cdd6f4",
    "titleBar.inactiveBackground": "#1e1e2e",
    "titleBar.inactiveForeground": "#6c7086",
    
    "panel.background": "#181825",
    "panel.foreground": "#cdd6f4",
    "panelTitle.activeForeground": "#f9e2af",
    "panelTitle.inactiveForeground": "#6c7086",
    
    "terminal.background": "#1e1e2e",
    "terminal.foreground": "#cdd6f4",
    "terminal.ansiBlack": "#45475a",
    "terminal.ansiRed": "#f38ba8",
    "terminal.ansiGreen": "#a6e3a1",
    "terminal.ansiYellow": "#f9e2af",
    "terminal.ansiBlue": "#89b4fa",
    "terminal.ansiMagenta": "#f5c2e7",
    "terminal.ansiCyan": "#94e2d5",
    "terminal.ansiWhite": "#cdd6f4",
    "terminal.ansiBrightBlack": "#6c7086",
    "terminal.ansiBrightRed": "#eba0ac",
    "terminal.ansiBrightGreen": "#94e2d5",
    "terminal.ansiBrightYellow": "#f2cdcd",
    "terminal.ansiBrightBlue": "#89dceb",
    "terminal.ansiBrightMagenta": "#f5e0de",
    "terminal.ansiBrightCyan": "#a6e3a1",
    "terminal.ansiBrightWhite": "#bac2de"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6c7086",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f38ba8"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89dceb"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f38ba8"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable.use", "string.other.link"],
      "settings": {
        "foreground": "#94e2d5"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "entity.other.attribute-name.class",
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#94e2d5"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "CSS Attributes",
      "scope": ["source.css support.type.property-name"],
      "settings": {
        "foreground": "#94e2d5"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#f2cdcd"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#f2cdcd"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "meta.structure.dictionary.json string.quoted.double.json - meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json string.quoted.double.json - meta.structure.dictionary.value.json meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#94e2d5"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.json string.quoted.double.json - meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.json string.quoted.double.json - meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": "#89b4fa",
    "method": "#89b4fa",
    "class": "#f9e2af",
    "interface": "#f9e2af",
    "type": "#f9e2af",
    "parameter": "#f5c2e7",
    "variable": "#f5c2e7",
    "property": "#94e2d5",
    "enum": "#f9e2af",
    "enumMember": "#fab387",
    "keyword": "#cba6f7",
    "namespace": "#f2cdcd",
    "event": "#f38ba8",
    "string": "#a6e3a1",
    "number": "#fab387",
    "regexp": "#f2cdcd",
    "operator": "#89dceb",
    "decorator": "#cba6f7",
    "typeParameter": "#f2cdcd",
    "macro": "#f38ba8"
  }
}